# 🌐 Port used by Express server
PORT=5000

# 🧠 Secret key for sessions, JWTs, etc.
SESSION_SECRET=your_super_secure_session_secret

# 🛢️ Database URL (if you're using one like PostgreSQL)
DATABASE_URL=your_database_connection_url

# 🔗 Base URL for API used in React app (must start with VITE_)
VITE_API_BASE_URL=https://your-app-name.onrender.com/api
